{
  "id": "midi-qol",
  "title": "Midi QOL",
  "description": "Automates rolling weapon/spell attacks and applying damage",

  "version": "10.0.18",
  "authors": [{ "name": "tposney" }],
  "socket": true,
  "scripts": [],
  "esmodules": ["src/midi-qol.js"],
  "styles": ["css/styles.css"],
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "compatibility": {
          "minimum": "2.0.3",
          "compatible": "2.0.3"
        }
      }
    ],
    "requires": [
      {
        "id": "socketlib",
        "type": "module"
      },
      {
        "id": "lib-wrapper",
        "type": "module"
      }
    ]
  },
  "packs": [
    {
      "name": "midiqol-sample-items",
      "label": "MidiQOL Sample Items",
      "path": "./packs/sampleitems.db",
      "module": "world",
      "type": "Item",
      "system": "dnd5e",
      "lang": ["en", "de"]
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "ja",
      "name": " 日本語",
      "path": "lang/ja.json"
    },
    {
      "lang": "ko",
      "name": "Korean",
      "path": "lang/ko.json"
    },
    {
      "lang": "fr",
      "name": "Français",
      "path": "lang/fr.json"
    },
    {
      "lang": "pt-BR",
      "name": "Português (Brasil)",
      "path": "lang/pt-BR.json"
    },
    {
      "lang": "es",
      "name": "Spanish",
      "path": "lang/es.json"
    },
    {
      "lang": "de",
      "name": "Deutsch",
      "path": "lang/de.json"
    },
    {
      "lang": "it",
      "name": "Italian",
      "path": "lang/it.json"
    }
  ],
  "compatibility": {
    "minimum": 10.286,
    "verified": 10,
    "maximum": 10
  },
  "bugs": "https://gitlab.com/tposney/midi-qol/-/issues",
  "flags": {
    "allowBugReporter": true
  },
  "url": "https://gitlab.com/tposney/midi-qol",
  "manifest": "https://gitlab.com/tposney/midi-qol/raw/v10/package/module.json",
  "download": "https://gitlab.com/tposney/midi-qol/raw/v10/package/midi-qol-v10.0.18.zip",
  "changelog": "https://gitlab.com/tposney/midi-qol/raw/v10/Changelog.md"
